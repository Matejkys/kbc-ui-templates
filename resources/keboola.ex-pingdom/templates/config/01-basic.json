{
  "name": "Basic - last 24 hours",
  "description": "Downloads all\n\n - checks and their results \n - probes \n - alerts \n - contacts \n - settings\n - credits",
  "data": {
    "incrementalOutput": true,
    "jobs": [
      {
        "endpoint": "settings",
        "dataField": "settings",
        "dataType": "settings"
      },
      {
        "endpoint": "credits",
        "dataField": "credits",
        "dataType": "credits"
      },
      {
        "endpoint": "probes",
        "params": {
          "includedeleted": true
        },
        "dataField": "probes",
        "dataType": "probes"
      },
      {
        "endpoint": "contacts",
        "dataField": "contacts",
        "dataType": "contacts"
      },
      {
        "endpoint": "checks",
        "dataType": "checks",
        "children": [
          {
            "endpoint": "checks/{1:id}",
            "placeholders": {
              "1:id": "id"
            },
            "dataField": "check",
            "dataType": "checks-detailed"
          },
          {
            "endpoint": "results/{1:id}",
            "params": {
              "from": {
                "function": "strtotime",
                "args": [
                  {
                    "attr": "loadFrom"
                  }
                ]
              },
              "to": {
                "function": "time"
              },
              "limit": 1000
            },
            "placeholders": {
              "1:id": "id"
            },
            "dataField": "results",
            "dataType": "results"
          }
        ]
      },
      {
        "endpoint": "actions",
        "dataField": "actions.alerts",
        "dataType": "alerts"
      }
    ],
    "mappings": {
      "contacts": {
        "id": {
          "mapping": {
            "destination": "id",
            "primaryKey": true
          }
        },
        "name": {
          "mapping": {
            "destination": "name"
          }
        },
        "email": {
          "mapping": {
            "destination": "email"
          }
        },
        "cellphone": {
          "mapping": {
            "destination": "cellphone"
          }
        },
        "countryiso": {
          "mapping": {
