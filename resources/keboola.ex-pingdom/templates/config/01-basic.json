          }
        },
        "directtwitter": {
          "mapping": {
            "destination": "directtwitter"
          }
        },
        "twitteruser": {
          "mapping": {
            "destination": "twitteruser"
          }
        },
        "paused": {
          "mapping": {
            "destination": "paused"
          }
        }
      },
      "checks": {
