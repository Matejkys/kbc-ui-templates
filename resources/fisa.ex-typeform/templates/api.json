{
    "baseUrl": "https://api.typeform.com/",
    "http": {
        "headers": {
            "Content-Type": "application/json",
            "Accept": "application/json",
            "Authorization": {
                "function": "concat",
                "args": [
                    "Bearer ",
                    {
                        "attr": "#token"
                    }
                ]
            }
        }
    },
    "pagination": {
        "method": "multiple",
        "scrollers": {
            "pagenum": {
                "method": "pagenum"
            }
        }
    }
}