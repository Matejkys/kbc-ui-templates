{
  "baseUrl": "https://api.linkedin.com/v2/",
  "authentication": {
    "type": "oauth20",
    "format": "json",
    "headers": {
      "Authorization": {
        "function": "concat",
        "args": [
          "Bearer ",
          {
            "authorization": "data.access_token"
          }
        ]
      }
    }
  }
}
