{
    "name": "Ads Reporting - Viral",
    "description": "Endpoint: adAnalyticsV2 - Viral",
    "data":{
        "jobs":[
            {
                "endpoint": "adAnalyticsV2",
                "params":{
                    "q": "analytics",
                    "pivot": {
                        "attr": "pivot"
                    },
                    "timeGranularity":{
                        "attr": "timeGranularity"
                    },
                    "dateRange.start.year":{
                        "attr": "start_year"
                    },
                    "dateRange.start.month":{
                        "attr": "start_month"
                    },
                    "dateRange.start.day":{
                        "attr": "start_day"
                    },
                    "campaigns[0]":{
                        "function":"concat",
                        "args":[
                            "urn:li:sponsoredCampaign:",
                            {"attr":"campaign_id"}
                        ]
                    }
                },
                "dataField": "elements"
            }
        ],
        "mappings":{
