[
  {
    "endpoint": "orgs/keboola/members",
    "dataType": "members"
  },
  {
    "endpoint": "orgs/keboola/teams",
    "dataType": "teams"
  },
  {
    "endpoint": "orgs/keboola/repos",
    "dataType": "repos",
    "children": [
      {
        "endpoint": "repos/keboola/{1:name}/commits",
        "recursionFilter": "size>=1",
        "placeholders": {
          "1:name": "name"
        },
        "dataType": "commits",
        "params": {
          "since": {
            "function": "date",
            "args": [
              "Y-m-d",
              {
                "time": "previousStart"
              }
            ]
          }
        }
      },
      {
        "endpoint": "repos/keboola/{1:name}/issues",
        "placeholders": {
          "1:name": "name"
        },
        "dataType": "issues",
        "params": {
          "since": {
            "function": "date",
            "args": [
              "Y-m-d",
              {
                "time": "previousStart"
              }
            ]
          }
        }
      }
    ]
  }
]
