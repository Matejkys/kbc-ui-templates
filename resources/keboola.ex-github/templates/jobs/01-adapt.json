[
        {
          "endpoint": "user/orgs",
          "dataType": "organizations",
          "children": [
            {
              "endpoint": "orgs/{1:login}/members",
              "dataType": "members",
              "placeholders": {
                "1:login": "login"
              }
            },
            {
              "endpoint": "orgs/{1:login}/teams",
              "dataType": "teams",
              "placeholders": {
                "1:login": "login"
              }
            },
            {
              "endpoint": "orgs/{1:login}/repos",
              "dataType": "repos",
              "placeholders": {
                "1:login": "login"
              },
              "children": [
                {
                  "endpoint": "repos/{2:login}/{1:name}/commits",
                  "recursionFilter": "size>=1",
                  "placeholders": {
                    "1:name": "name",
                    "2:login": "login"
                  },
                  "dataType": "commits",
                  "params": {
                    "since": {
                      "function": "date",
                      "args": [
                        "Y-m-d",
                        {
                          "time": "previousStart"
                        }
                      ]
                    }
                  }
                },
                {
                  "endpoint": "repos/{2:login}/{1:name}/issues",
                  "placeholders": {
                    "1:name": "name",
                    "2:login": "login"
                  },
                  "dataType": "issues",
                  "params": {
                    "since": {
                      "function": "date",
                      "args": [
                        "Y-m-d",
                        {
                          "time": "previousStart"
                        }
                      ]
                    }
                  }
                }
              ]
            }
          ]
        }
      ]