{
   "baseUrl": "https://api.surveymonkey.net/v3/",
   "authentication": {
    "type": "query",
    "query": {
     "api_key": {
      "attr": "api_key"
     }
    }
   },
   "pagination": {
    "method": "response.url",
    "urlKey": "links.next",
    "limitStop": {
     "field": "total"
    }
   },
   "http": {
    "requiredHeaders": [
     "authorization",
     "Content-Type"
    ],
      "headers": {
          "authorization": {
              "function": "concat",
              "args": [
                  {
                      "attr": "bearer"
                  },
                  {
                      "attr": "#secret"
                  }
              ]
          },
          "Content-Type": "application/json"
      }
   }
}
