{
  "name": "01 - Basic",
  "description": "Downloads \n\n - work reports from specific date",
  "data": {
    "incrementalOutput": true,
    "jobs": [
      {
        "endpoint": "work-reports",
        "dataField": "data.reports",
        "dataType": "work-reports",
        "params": {
          "date_reported_range[date_from]": {
            "function": "date",
            "args": [
              "Y-m-d",
              {
                "function": "strtotime",
                "args": [
                  {
                    "attr": "since"
                  },
                  {
                    "time": "currentStart"
                  }
                ]
              }
            ]
          }
        }
      }
    ],
    "mappings": {
      "work-reports": {
        "id": {
          "type": "column",
          "mapping": {
            "destination": "id",
            "primaryKey": true
          }
        },
        "date_add": {
          "type": "column",
          "mapping": {
            "destination": "date_add"
          }
        },
        "date_reported": {
          "type": "column",
          "mapping": {
            "destination": "date_reported"
          }
        },
        "note": {
          "type": "column",
          "mapping": {
            "destination": "note"
          }
        },
        "minutes": {
          "type": "column",
          "mapping": {
            "destination": "minutes"
          }
        },
        "cost.amount": {
          "type": "column",
          "mapping": {
            "destination": "cost_amount"
          }
        },
        "cost.currency": {
          "type": "column",
          "mapping": {
            "destination": "cost_currency"
          }
        },
        "task.id": {
          "type": "column",
          "mapping": {
            "destination": "task_id"
          }
        },
        "task.name": {
          "type": "column",
          "mapping": {
            "destination": "task_name"
          }
        },
        "tasklist.id": {
          "type": "column",
          "mapping": {
            "destination": "tasklist_id"
          }
        },
        "tasklist.name": {
          "type": "column",
          "mapping": {
            "destination": "tasklist_name"
          }
        },
        "project.id": {
          "type": "column",
          "mapping": {
            "destination": "project_id"
          }
        },
        "project.name": {
          "type": "column",
          "mapping": {
            "destination": "project_name"
          }
        },
        "project.labels": {
          "type": "column",
          "forceType": true,
          "mapping": {
            "destination": "project_labels"
          }
        },
        "author.id": {
          "type": "column",
          "mapping": {
            "destination": "author_id"
          }
        },
        "author.fullname": {
          "type": "column",
          "mapping": {
            "destination": "author_fullname"
          }
        },
        "worker.id": {
          "type": "column",
          "mapping": {
            "destination": "worker_id"
          }
        },
        "worker.fullname": {
          "type": "column",
          "mapping": {
            "destination": "worker.fullname"
          }
        }
      }
    }
  }
}