{
  "baseUrl": {
    "function": "sprintf",
    "args": [
      "https://quickbooks.api.intuit.com/v3/company/%s/",
      {
        "attr": "companyid"
      }
    ]
  },
  "authentication": {
    "type": "oauth20",
    "format": "json",
    "headers": {
      "Authorization": {
        "function": "concat",
        "args": [
          "Bearer ",
          {
            "authorization": "data.access_token"
          }
        ]
      }
    }
  },
  "http": {
    "headers": {
      "Accept": "application/json"
    }
  }
}