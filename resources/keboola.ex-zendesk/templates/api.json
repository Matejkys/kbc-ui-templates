{
  "baseUrl": {
    "function": "concat",
    "args": [
      "https://",
      {
        "attr": "domain"
      },
      ".zendesk.com/api/v2/"
    ]
  },
  "authentication": {
    "type": "oauth20",
    "format": "json",
    "headers": {
      "Authorization": {
        "function": "concat",
        "args": [
          "Bearer ",
          {
            "authorization": "data.access_token"
          }
        ]
      }
    }
  },
  "http": {
    "headers": {
      "Accept": "application/json"
    }
  },
  "pagination": {
    "method": "zendesk.response.url"
  }
}
