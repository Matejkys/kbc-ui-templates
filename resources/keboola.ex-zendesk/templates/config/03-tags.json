{
  "name": "Tags",
  "description": "Downloads all *tags*.",
  "data": {
    "jobs": [
      {
        "endpoint": "tags.json",
        "dataType": "tags"
      }
    ],
    "mappings": {
      "tags": {
        "name": {
          "mapping": {
            "destination": "name"
          }
        },
        "count": {
          "mapping": {
            "destination": "count"
          }
        }
      }
    }
  }
}
