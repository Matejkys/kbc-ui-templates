{
    "api": {
        "baseUrl": {
            "function": "concat",
            "args": [
                "https://api.visioncritical.com/v1/applications/", {
                    "attr": "community"
                },
                "/"
            ]
        },
        "pagination": {
            "method": "offset",
            "limit": 1000
        }
    },
    "config": {
        "debug": 1,
        "email": "{YOUR EMAIL}",
        "#password": "{YOUR PASSWORD}",
        "community": "{YOUR COMMUNITY}",
        "http": {
            "headers": {
                "Accept": "application/json",
                "cache-control": "no-cache",
                "authorization": {
                    "function": "concat",
                    "args": [
                        "Basic ", {
                            "function": "base64_encode",
                            "args": [{
                                "function": "concat",
                                "args": [{
                                        "attr": "email"
                                    },
                                    ":", {
                                        "attr": "#password"
                                    }
                                ]
                            }]
                        }
                    ]
                }
            }
        }
    }
}
".": {
    "mapping": {
        "destination": "variable_categories"
    }
}
}
},
