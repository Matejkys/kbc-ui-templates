{
   "name":"Mapped Template",
   "description":"Basic template extracts all Webinars and its details from your Zoom Account and uses Storage mapping",
   "data":{
      "jobs":[
         {
            "endpoint":"users",
            "dataType":"users",
            "children":[
               {
                  "endpoint":"users/{user-id}/webinars",
                  "dataType":"webinars",
                  "dataField":"webinars",
                  "placeholders":{
                     "user-id":"id"
                  },
                  "children":[
                     {
                        "endpoint":"webinars/{webinar-id}/registrants",
                        "dataField":"registrants",
                        "dataType":"webinars-registrants",
                        "placeholders":{
                           "webinar-id":"id"
                        }
                     },
                     {
                        "endpoint":"webinars/{webinar-id}/panelists",
                        "dataField":"panelists",
                        "dataType":"webinars-panelists",
                        "placeholders":{
                           "webinar-id":"id"
                        }
                     },
                     {
                        "endpoint":"webinars/{webinar-id}",
                        "dataType":"webinars-detail",
                        "dataField":".",
                        "placeholders":{
                           "webinar-id":"id"
                        }
                     },
                     {
                        "endpoint":"webinars/{webinar-id}/polls",
                        "dataField":"polls",
                        "dataType":"webinars-polls",
                        "placeholders":{
                           "webinar-id":"id"
                        }
                     },
                     {
                        "endpoint":"past_webinars/{webinar-id}/instances",
                        "dataField":"webinars",
                        "dataType":"webinars-past",
                        "placeholders":{
                           "webinar-id":"id"
                        },
                        "children":[
                           {
                              "endpoint":"past_webinars/{webinar-uuid}/absentees",
                              "dataField":"registrants",
                              "dataType":"webinars-past-absentees",
                              "placeholders":{
                                 "webinar-uuid":"uuid"
                              }
                           },
                           {
                              "endpoint":"past_webinars/{webinar-uuid}/polls",
                              "dataField":"questions",
                              "dataType":"webinars-past-polls",
                              "placeholders":{
                                 "webinar-uuid":"uuid"
                              }
                           },
                           {
                              "endpoint":"past_webinars/{webinar-uuid}/qa",
                              "dataField":"questions",
                              "dataType":"webinars-past-qa",
                              "placeholders":{
                                 "webinar-uuid":"uuid"
                              }
                           },
                           {
                              "endpoint":"metrics/webinars/{webinar-uuid}/participants?type=past",
                              "dataType":"webinars-past-participants",
                              "dataField":"participants",
                              "placeholders":{
                                 "webinar-uuid":"uuid"
                              }
                           }
                        ]
                     }
                  ]
               }
            ]
         }
      ],
