                        "start": 1
                    },
                    "placeholders": {
                        "ObjectID": "_refObjectUUID"
                    }
                }]
            },
            {
                "endpoint": "portfolioitem/theme",
                "dataType": "theme",
                "dataField": "QueryResult.Results",
                "params": {
                    "start": 1
                },
                "children": [{
                    "endpoint": "portfolioitem/theme/{ObjectID}/children",
                    "dataType": "theme_detail",
