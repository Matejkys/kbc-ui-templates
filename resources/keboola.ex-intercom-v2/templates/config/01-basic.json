{
    "name": "Full (events summary)",
    "description": "Downloads all\n\n - users and their notes and summary of events\n - contacts\n - companies\n - admins\n - tags \n - segments\n - conversations and conversation parts",
    "data": {
        "jobs": [
            {
                "endpoint": "users/scroll",
                "scroller": "users",
                "dataType": "users",
                "children": [
                    {
                        "endpoint": "notes?user_id={1:user_id}",
                        "dataType": "notes",
                        "placeholders": {
                            "1:user_id": "user_id"
                        },
                        "recursionFilter": "user_id!=&email!="
                    },
                    {
                        "endpoint": "events?user_id={1:user_id}&type=user&summary=true",
                        "dataType": "events_summary",
                        "placeholders": {
                            "1:user_id": "user_id"
                        },
                        "recursionFilter": "user_id!=&email!="
                    }
                ]
            },
            {
                "endpoint": "contacts/scroll",
                "scroller": "contacts",
                "dataType": "contacts"
            },
            {
                "endpoint": "companies/scroll",
                "scroller": "companies",
                "dataType": "companies"
            },
            {
                "endpoint": "admins"
            },
            {
                "endpoint": "tags"
            },
            {
                "endpoint": "segments"
            },
            {
                "endpoint": "conversations",
                "params": {
                    "per_page": "60",
                    "display_as": "plaintext"
                },
                "children": [
                    {
                        "endpoint": "conversations/{1:conversation_id}",
                        "placeholders": {
                            "1:conversation_id": "id"
                        },
                        "params": {
                            "display_as": "plaintext"
                        },
                        "dataType": "conversation_parts",
                        "dataField": "conversation_parts.conversation_parts"
                    }
                ]
            }
        ]
    }
}
