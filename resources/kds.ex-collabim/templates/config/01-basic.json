{
    "name": "Basic",
    "description": "Downloads \n\n - Keywords \n - Positions",
    "data": {
        "incrementalOutput": true,
        "jobs": [
            {
                "endpoint": "keywords",
                "dataField": "data",
                "dataType": "keywords",
                "params": {
                    "projectId": {
                        "attr": "projectId"
                    },
                    "tags": {
                        "attr": "tags"
                    }
                },
                "children": [
                    {
                        "dataField": "data",
                        "dataType": "keywords_ids",
                        "endpoint": "keyword-positions?projectKeywordIds={id}&showSearchEngineName=1",
                        "placeholders": {
                            "id": "id"
                        },
                        "params": {
                            "projectId": {
                                "attr": "projectId"
                            },
                            "tags": {
                                "attr": "tags"
                            },
                            "from": {
                                "function": "date",
                                "args": [
                                    "Y-m-d",
                                    {
                                        "function": "strtotime",
                                        "args": [
                                            {
                                                "attr": "from"
                                            }
                                        ]
                                    }
                                ]
                            },
                            "to": {
                                "function": "date",
                                "args": [
                                    "Y-m-d",
                                    {
                                        "function": "strtotime",
                                        "args": [
                                            {
                                                "attr": "to"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        ],
        "mappings": {
            "keywords": {
                "id": {
                    "mapping": {
                        "destination": "keyword_id",
                        "primaryKey": true
                    }
                },
                "keyword": "keyword"
            },
            "keywords_ids": {
                "projectKeywordId": {
                    "mapping": {
                        "destination": "keyword_id",
                        "primaryKey": true
                    }
                },
                "positions": {
                    "type": "table",
                    "destination": "keyword_positions",
                    "parentKey": {
                        "primaryKey": true,
                        "destination": "keyword_id"
                    },
                    "tableMapping": {
                        "searchEngineId": {
                            "type": "column",
                            "mapping": {
                                "destination": "search_engine",
                                "primaryKey": true
                            }
                        },
                        "date": {
                            "type": "column",
                            "mapping": {
                                "destination": "date",
                                "primaryKey": true
                            }
                        },
                        "position": {
                            "type": "column",
                            "mapping": {
                                "destination": "position"
                            }
                        }
                    }
                }
            }
        }
    }
}
